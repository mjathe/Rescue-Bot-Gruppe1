AutoMode()
{
if(findObstacle(person) == true)
{
  engine.transmission(0)
  rescue()
}
else
{
  AudioNavigation();

  if (ultrasonic == true)
  {
    engine.transmission(0)
    avoidObstacle()
  }else if(findObstacle(branch)== true)
  {
    engine.transmission(0)
    callOperator
    activateChainSaw()
  }
  else
  {
    engine.transmission(1)
  }
}
}
driveToPerson()
{
  while(Operator != PressStop)
  {
  AutoMode()
  }
}
