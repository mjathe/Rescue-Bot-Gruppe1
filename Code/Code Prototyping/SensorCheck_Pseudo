}
checkObstacle()
{
//vornelinks = 0
//vorneRechts = 1
//hintenRechts = 2
//hintenLinks = 3
obstacle[0]= SensorRead(vornelinks)
obstacle[1]= SensorRead(vorneRechts)
obstacle[2]= SensorRead(hintenRechts)
obstacle[3]= SensorRead(hintenLinks)


}
